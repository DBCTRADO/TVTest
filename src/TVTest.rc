			MENUITEM SEPARATOR
			MENUITEM "お気に入りに追加(&A)", CM_ADDTOFAVORITES
			MENUITEM "お気に入りの管理(&O)...", CM_ORGANIZEFAVORITES
		END
		POPUP "最近見たチャンネル(&H)"
		BEGIN
			MENUITEM "なし", 0, GRAYED
		END
		MENUITEM "ワンセグモード(&1)", CM_1SEGMODE
		MENUITEM SEPARATOR
		POPUP "音量(&V)"
		BEGIN
			MENUITEM "消音(&M)", CM_VOLUME_MUTE
			MENUITEM "上げる(&U)", CM_VOLUME_UP
			MENUITEM "下げる(&D)", CM_VOLUME_DOWN
