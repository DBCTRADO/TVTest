		END
		POPUP "プラグイン(&U)"
		BEGIN
			MENUITEM "なし", 0, GRAYED
		END
		MENUITEM "設定(&O)...", CM_OPTIONS
		POPUP "フィルタのプロパティ(&Y)"
		BEGIN
			MENUITEM "映像デコーダ(&D)...", CM_VIDEODECODERPROPERTY
			MENUITEM "映像レンダラ(&R)...", CM_VIDEORENDERERPROPERTY
