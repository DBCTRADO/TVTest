			MENUITEM "音量増幅なし(&N)", CM_AUDIOGAIN_NONE
			MENUITEM "音量増幅(x1.25)", CM_AUDIOGAIN_125
			MENUITEM "音量増幅(x1.5)", CM_AUDIOGAIN_150
			MENUITEM "音量増幅(x2.0)", CM_AUDIOGAIN_200
			MENUITEM SEPARATOR
			MENUITEM "5.1ch音量増幅なし(&S)", CM_SURROUNDAUDIOGAIN_NONE
			MENUITEM "5.1ch音量増幅(x1.25)", CM_SURROUNDAUDIOGAIN_125
			MENUITEM "5.1ch音量増幅(x1.5)", CM_SURROUNDAUDIOGAIN_150
			MENUITEM "5.1ch音量増幅(x2.0)", CM_SURROUNDAUDIOGAIN_200
		END
		POPUP "音声(&M)"
		BEGIN
			MENUITEM "", 0, GRAYED
		END
		POPUP "映像切替(&Q)"
		BEGIN
			MENUITEM "", 0, GRAYED
		END
		MENUITEM SEPARATOR
		MENUITEM "録画開始/停止(&R)", CM_RECORD
		MENUITEM "録画(詳細指定)(&D)...", CM_RECORDOPTION
		MENUITEM SEPARATOR
		MENUITEM "画像のコピー(&P)\tC", CM_COPYIMAGE
		MENUITEM "画像の保存(&J)\tV", CM_SAVEIMAGE
		MENUITEM "キャプチャウィンドウ(&X)", CM_CAPTUREPREVIEW
		MENUITEM SEPARATOR
		MENUITEM "再生オフ(&I)", CM_DISABLEVIEWER
		POPUP "リセット(&E)"
		BEGIN
			MENUITEM "リセット(&R)", CM_RESET
			MENUITEM "ビューアリセット(&V)", CM_RESETVIEWER
			MENUITEM "バッファリセット(&B)", CM_RESETBUFFER
			MENUITEM SEPARATOR
			MENUITEM "フィルタグラフ再初期化(&F)", CM_REBUILDVIEWER
		END
		MENUITEM SEPARATOR
		MENUITEM "パネルウィンドウ(&N)", CM_PANEL
		MENUITEM "EPG番組表(&G)", CM_PROGRAMGUIDE
		POPUP "バー/枠(&B)"
		BEGIN
			MENUITEM "タイトルバー(&T)", CM_TITLEBAR
			MENUITEM "ステータスバー(&S)", CM_STATUSBAR
			MENUITEM "サイドバー(&I)", CM_SIDEBAR
			MENUITEM SEPARATOR
			MENUITEM "ウィンドウ枠 : 通常(&F)", CM_WINDOWFRAME_NORMAL
			MENUITEM "ウィンドウ枠 : 細くする(&H)", CM_WINDOWFRAME_CUSTOM
			MENUITEM "ウィンドウ枠 : なくす(&N)", CM_WINDOWFRAME_NONE
			MENUITEM SEPARATOR
			MENUITEM "独自タイトルバーを使用(&C)", CM_CUSTOMTITLEBAR
			MENUITEM "パネルドッキング時にタイトルバーを分割(&P)", CM_SPLITTITLEBAR
		END
		POPUP "プラグイン(&U)"
		BEGIN
			MENUITEM "なし", 0, GRAYED
		END
		MENUITEM "設定(&O)...", CM_OPTIONS
		POPUP "フィルタのプロパティ(&Y)"
		BEGIN
			MENUITEM "映像デコーダ(&D)...", CM_VIDEODECODERPROPERTY
			MENUITEM "映像レンダラ(&R)...", CM_VIDEORENDERERPROPERTY
			MENUITEM "音声フィルタ(&A)...", CM_AUDIOFILTERPROPERTY
			MENUITEM "音声レンダラ(&U)...", CM_AUDIORENDERERPROPERTY
			MENUITEM "MPEG-2 Demultiplexer(&M)...", CM_DEMULTIPLEXERPROPERTY
		END
		MENUITEM "ストリームの情報(&K)", CM_STREAMINFO
		MENUITEM SEPARATOR
		MENUITEM "閉じる(&X)", CM_CLOSE
	END
END

